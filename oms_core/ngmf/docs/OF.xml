        <row>
          <entry><code>ABSDIF</code></entry>

          <entry>Absolute difference</entry>

          <entry><inlineequation>
              <m:math display="inline">
                <m:mrow>
                  <m:munderover>
                    <m:mo>∑</m:mo>

                    <m:mi>i=1</m:mi>

                    <m:mi>n</m:mi>
                  </m:munderover>

                  <m:mrow>
                    <m:menclose notation="left right">
                      <m:mrow>
                        <m:msub>
                          <m:mi>Q</m:mi>

                          <m:mi>i,o</m:mi>
                        </m:msub>

                        <m:mo>-</m:mo>

                        <m:msub>
                          <m:mi>Q</m:mi>

                          <m:mi>i,s</m:mi>
                        </m:msub>
                      </m:mrow>
                    </m:menclose>
                  </m:mrow>
                </m:mrow>
              </m:math>
            </inlineequation></entry>
        </row>

